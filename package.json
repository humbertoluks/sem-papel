{
  "name": "sempapel",
  "version": "1.0.0",
  "main": "",
  "author": "Humberto Luksevicius",
  "license": "MIT",
  "scripts": {
    "start": "dotnet run -p ./src/Backend/Backend.csproj",
    "publish-project": "cd ./src/Backend/ && dotnet publish -o D:/temp/publish/Sem-Papel/Backend",
    "watch": "dotnet watch -p ./src/Backend/Backend.csproj run",
    "migration": "cd ./src/Repository/ && dotnet ef --startup-project ../Backend/Backend.csproj migrations add ",
    "remove-migration":"cd ./src/Repository/ && dotnet ef --startup-project ../Backend/Backend.csproj migrations remove",
    "database-update":"cd ./src/Repository/ && dotnet ef  --startup-project ../Backend/Backend.csproj database update"
  }
}
